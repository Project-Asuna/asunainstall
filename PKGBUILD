pkgname=asunainstall
pkgver=0.6
pkgrel=1
pkgdesc="Installer for Project Asuna"
arch=(x86_64)
url="https://asunaproject.nl"
source=(asunainstall)
sha256sums=('19860abb1a288e65088fb298ac71b00ce6f37adca4e93d1832342b535613f424')

package() {
  install -Dm755 asunainstall "$pkgdir"/usr/bin/asunainstall
}
